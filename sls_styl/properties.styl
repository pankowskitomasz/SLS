set-mb-property(propertyA,valueA)
    {propertyA}: valueA
    -o-{propertyA}: valueA
    -moz-{propertyA}: valueA
    -webkit-{propertyA}: valueA


set-calc-property(propertyA,valueH,valueL)
    {propertyA}: "calc(%s + %s)" % (valueH valueL)
    {propertyA}: "-o-calc(%s + %s)" % (valueH valueL)
    {propertyA}: "-moz-calc(%s + %s)" % (valueH valueL)
    {propertyA}: "-webkit-calc(%s + %s)" % (valueH valueL)


set-prop-series(classNameA='',propertyA='',prefixMulA=1,mbA=0,startA=0,endA=0,stepA=0)
    if(classNameA != "" && propertyA != "" && prefixMulA >= 1 && startA < endA)
        for i in range(startA,endA)
            .{classNameA}-{i * prefixMulA} 
                if(mbA == 0)
                    {propertyA}: i * stepA
                else
                    set-mb-property(propertyA,i * stepA)
